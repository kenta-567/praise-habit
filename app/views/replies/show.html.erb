<div class="conteiner">
  <div class="reply-heading">
    <h1>Praise To Me</h1>
  </div>
  <div class="reply-box">
    <div class="reply-content">
      <div class="reply-content-heading">
        <h2>Praise</h2>
      </div>
      <div class="post-list">
        <% @receive.each do |messege| %>
          <div class="post-item">
            <lavel>From<%= messege.user.nickname %></lavel>
            <text><%= messege.post_content %></text>
            <label><%= time_ago_in_words(messege.created_at) %></label>
          </div>
        <% end %>
      </div>
    </div>
    <div class="thanks-content">
      <div class="thanks-content-heading">
        <h2>Thanks</h2>
      </div>
    </div>
  </div>
</div>
